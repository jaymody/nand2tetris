/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=nsel);
    And(a=nsel, b=a, out=ra);
    And(a=sel, b=b, out=rb);
    Or(a=ra, b=rb, out=out);
}
